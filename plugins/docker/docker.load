

function dip -d "Find the IPadress of a running docker container"
  docker inspect --format '{{ .NetworkSettings.IPAddress }}' $argv[1]
end
function dki -d "Run a docker container interactively"
  docker run -t -i -P $argv
end
function dkd -d "Run a docker container in daemon mode"
  docker run -d -P $argv
end
function dl -d "Get the id of the last run container"
  docker ps -l -q
end

